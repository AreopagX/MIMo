<mujoco model="mmimo">

    <compiler inertiafromgeom="true" angle="degree"/>

    <default>
        <joint limited="true" damping="1.5" stiffness="1" armature="0.1" solimplimit="0.98 0.99 0.001 .5 1" solreflimit="0.02 1"/>
        <geom condim="3" material="skin" friction="1 0.005 0.0002" solimp="0.95 0.99 0.001 0.5 1" solref="0.005 1"/>
        <motor ctrlrange="-.4 .4" ctrllimited="true"/>
		<camera user="0 0 0"/>
    </default>

    <option timestep="0.005" iterations="50" tolerance="1e-10" solver="Newton" jacobian="dense" cone="elliptic" impratio="1.5"/>

    <size nconmax="1000" njmax="5000" nstack="10000000" nuser_cam="3"/>

    <visual>
        <map force="0.1" zfar="30" znear="0.005"/>
        <rgba haze="0.15 0.25 0.35 1"/>
        <quality shadowsize="4096"/>
        <global offwidth="800" offheight="800"/>
    </visual>

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="512"/>

        <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>

        <texture name="texgeom" type="cube" builtin="flat" mark="cross" width="127" height="1278"
            rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" markrgb="1 1 1" random="0.01"/>
			
		<!--<texture name="tex_head" type="cube" fileright="tex/primitives_right.png" fileleft="tex/primitives_left.png" fileup="tex/primitives_up.png" filedown="tex/primitives_down.png" filefront="tex/primitives_front.png" fileback="tex/primitives_back.png" rgb1="0.95 .8 .5"/>-->
		<texture name="tex_head" type="cube" fileright="tex/head_front.png" fileleft="tex/head_back.png" fileup="tex/head_left.png" filedown="tex/head_right.png" filefront="tex/head_top.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_eye_left" type="cube" fileright="tex/eye_pupil_left.png" rgb1="1 1 1"/>
		<texture name="tex_eye_right" type="cube" fileright="tex/eye_pupil_right.png" rgb1="1 1 1"/>
		<!---
		<texture name="tex_top_up" type="cube" fileright="tex/primitives_right.png" fileleft="tex/primitives_left.png" fileup="tex/primitives_up.png" filedown="tex/primitives_down.png" filefront="tex/primitives_front.png" fileback="tex/primitives_back.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_top_down" type="cube" fileright="tex/primitives_right.png" fileleft="tex/primitives_left.png" fileup="tex/primitives_up.png" filedown="tex/primitives_down.png" filefront="tex/primitives_front.png" fileback="tex/primitives_back.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_top_sleeve" type="cube" fileright="tex/primitives_right.png" fileleft="tex/primitives_left.png" fileup="tex/primitives_up.png" filedown="tex/primitives_down.png" fileback="tex/primitives_back.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_pants_up" type="cube" fileright="tex/primitives_right.png" fileleft="tex/primitives_left.png" fileup="tex/primitives_up.png" filedown="tex/primitives_down.png" filefront="tex/primitives_front.png" fileback="tex/primitives_back.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_pants_down" type="cube" fileright="tex/primitives_right.png" fileleft="tex/primitives_left.png" fileup="tex/primitives_up.png" filedown="tex/primitives_down.png" filefront="tex/primitives_front.png" fileback="tex/primitives_back.png" rgb1="0.95 .8 .5"/>
		<texture name="tex_pants_leg" type="cube" fileright="tex/primitives_right.png" fileleft="tex/primitives_left.png" fileup="tex/primitives_up.png" filedown="tex/primitives_down.png" filefront="tex/primitives_front.png" rgb1="0.95 .8 .5"/>
		
		-->

        <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="1 1" texuniform="true"/>

        <material name="matgeom" texture="texgeom" texuniform="true" rgba="0.8 0.6 .4 1"/>
		
		<material name="matmocap" texture="texgeom" texuniform="true" rgba="0.5 0 0.5 0.5"/>
		
		<material name="skin" rgba="0.95 0.8 0.5 1" specular=".15" shininess=".7"/>
		<material name="buttons" rgba=".05 .05 .05 1" specular=".9" shininess=".9"/>
		<material name="head" texture="tex_head" texuniform="true" specular=".0" shininess=".0"/>
		<material name="top_up" rgba=".1 .5 0.0 1" specular=".0" shininess=".0"/>
		<material name="top_middle" rgba=".1 .5 0.0 1" specular=".0" shininess=".0"/>
		<material name="top_down" rgba=".1 .5 0.0 1" specular=".0" shininess=".0"/>
		<material name="top_sleeve" rgba=".1 .5 0.0 1" specular=".0" shininess=".0"/>
