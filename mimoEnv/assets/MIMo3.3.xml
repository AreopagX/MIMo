		<pair geom1="lb" geom2="ub1" condim="1"/>
	</contact>
	
	
	<tendon>
		<fixed name="hip_bend" limited="true" range="-.01 .01">
			<joint joint="robot:hip_bend1" coef="1"/>
			<joint joint="robot:hip_bend2" coef="-1"/>
		</fixed>
		<fixed name="hip_rotation" limited="true" range="-.01 .01">
			<joint joint="robot:hip_rot1" coef="1"/>
			<joint joint="robot:hip_rot2" coef="-1"/>
		</fixed>
		<fixed name="hip_lean" limited="true" range="-.01 .01">
			<joint joint="robot:hip_lean1" coef="1"/>
			<joint joint="robot:hip_lean2" coef="-.5"/>
		</fixed>
		<fixed name="head_limits1" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="1"/>
		</fixed>
		<fixed name="head_limits2" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_tilt" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="1"/>
		</fixed>
		<fixed name="head_limits3" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt" coef="1"/>
		</fixed>
		<fixed name="head_limits1_sym" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="-1"/>
		</fixed>
		<fixed name="head_limits2_sym" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_tilt" coef="1"/>
			<joint joint="robot:head_tilt_side" coef="-1"/>
		</fixed>
		<fixed name="head_limits3_sym" limited="true" range="-2.1 2.1">
			<joint joint="robot:head_swivel" coef="1"/>
			<joint joint="robot:head_tilt" coef="-1"/>
		</fixed>
	</tendon>

    <actuator>
	
		<motor name="hip_bend" 					joint="robot:hip_bend1" 		gear="10.58" 	forcerange="-1 .765"/>
		<motor name="hip_twist" 				joint="robot:hip_rot1" 			gear="3.63" 	forcerange="-1 1"/>
		<motor name="hip_lean" 					joint="robot:hip_lean1" 		gear="7.25" 	forcerange="-1 1"/>
		
		<motor name="head_swivel" 				joint="robot:head_swivel" 		gear=".65" 		forcerange="-1 1"/>
		<motor name="head_tilt" 				joint="robot:head_tilt" 		gear="1.2" 		forcerange="-1 .55"/>
		<motor name="head_tilt_side" 			joint="robot:head_tilt_side" 	gear=".65" 		forcerange="-1 1"/>
		
		<motor name="left_eye_horizontal" 		joint="robot:left_eye_horizontal" 	gear=".1" 	forcerange="-1 1"/>
		<motor name="left_eye_vertical" 		joint="robot:left_eye_vertical" 	gear=".1" 	forcerange="-1 1"/>
		<motor name="right_eye_horizontal" 		joint="robot:right_eye_horizontal" 	gear=".1" 	forcerange="-1 1"/>
		<motor name="right_eye_vertical" 		joint="robot:right_eye_vertical" 	gear=".1" 	forcerange="-1 1"/>
		
		<motor name="right_shoulder_horizontal" joint="robot:right_shoulder_horizontal" gear="1.8" 	forcerange="-1 1"/>
		<motor name="right_shoulder_abduction" 	joint="robot:right_shoulder_ad_ab" 		gear="4" 	forcerange="-.6875 1"/>
		<motor name="right_shoulder_internal" 	joint="robot:right_shoulder_rotation" 	gear="2.5" 	forcerange="-.64 1"/>
		<motor name="right_elbow" 				joint="robot:right_elbow" 				gear="3.6" 	forcerange="-1 .83"/>
		<motor name="right_wrist_rotation" 		joint="robot:right_hand1" 				gear=".7"	forcerange="-1 1"/>
		<motor name="right_wrist_flexion" 		joint="robot:right_hand2" 				gear="1.4"	forcerange="-1 .5"/>
		<motor name="right_wrist_ulnar" 		joint="robot:right_hand3" 				gear=".7"	forcerange="-1 1"/>
		<motor name="right_fingers" 			joint="robot:right_fingers" 			gear=""		forcerange="-1 .33"/>
		
		<motor name="left_shoulder_horizontal" 	joint="robot:left_shoulder_horizontal" 	gear="1.8" 	forcerange="-1 1"/>
		<motor name="left_shoulder_abduction" 	joint="robot:left_shoulder_ab_ad" 		gear="4" 	forcerange="-.6875 1"/>
		<motor name="left_shoulder_internal" 	joint="robot:left_shoulder_rotation" 	gear="2.5" 	forcerange="-.64 1"/>
		<motor name="left_elbow" 				joint="robot:left_elbow" 				gear="3.6" 	forcerange="-1 .83"/>
		<motor name="left_wrist_rotation" 		joint="robot:left_hand1"  				gear=".7"	forcerange="-1 1"/>
		<motor name="left_wrist_flexion" 		joint="robot:left_hand2"  				gear="1.4"	forcerange="-1 .5"/>
		<motor name="left_wrist_ulnar" 			joint="robot:left_hand3"  				gear=".7"	forcerange="-1 1"/>
		<motor name="left_fingers" 				joint="robot:left_fingers" 				gear=".69" 	forcerange="-1 .33"/>
		
		<motor name="right_hip_flex" 			joint="robot:right_hip1" 		gear="8" 	forcerange="-1 1"/>
		<motor name="right_hip_abduction" 		joint="robot:right_hip2" 		gear="6.24" forcerange="-1 1"/>
		<motor name="right_hip_rotation" 		joint="robot:right_hip3" 		gear="3.54" forcerange="-.75 1"/>
		<motor name="right_knee" 				joint="robot:right_knee" 		gear="10" 	forcerange="-.65 1"/>
		<motor name="right_foot_flexion" 		joint="robot:right_foot1" 		gear="3.78"	forcerange="-1 .5"/>
		<motor name="right_foot_inversion" 		joint="robot:right_foot2" 		gear="2"	forcerange="-1 1"/>
		<motor name="right_foot_rotation" 		joint="robot:right_foot3"	 	gear="2"	forcerange="-1 1"/>
	
		<motor name="left_hip_flex" 			joint="robot:left_hip1" 		gear="8" 	forcerange="-1 1"/>
		<motor name="left_hip_abduction" 		joint="robot:left_hip2" 		gear="6.24" forcerange="-1 1"/>
